		<dataSource>DataSource1</dataSource>
		<query>// CurrencyRates
select Rates.Currency as Currency, Rates.Rate as Rate, Rates.Factor as Factor
into Rates
from InformationRegister.ExchangeRates.SliceLast ( , &amp;LocalCurrency ) as Rates
;
//
// Main selection
//
select &amp;StrTotalsGroup as StrTotalsGroup, &amp;StrPayment as StrPayment, &amp;StrOverduePayment as StrOverduePayment,
	Balances.PaymentKey as PaymentKey, Balances.Contract.Owner as Vendor,
	Balances.Contract as Contract, Balances.Document as Document,
	Balances.Detail as Detail, PaymentDetails.Date as PaymentDate, PaymentDetails.Option as Option,
	case when PaymentDetails.Date &lt; &amp;DebtsOnDate then datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) else 0 end as BalanceDays,
	case
		when datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) &lt;= 0 then 0
		when datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) &gt; &amp;PeriodsBorder then &amp;PeriodsBorder + 1
		else &amp;PeriodFactor * (
			case when cast ( datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) / &amp;PeriodFactor - 0.5 as Number ( 15 ) ) = datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) / &amp;PeriodFactor then 0
			else 1
			end + cast ( datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) / &amp;PeriodFactor - 0.5 as Number ( 15 ) )
		)
	end as OverduePeriods,
	case when &amp;LocalCurrency then Balances.PaymentBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.PaymentBalance
	end as PaymentBalance,
	case when &amp;LocalCurrency then Balances.AmountBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.AmountBalance
	end as AmountDebt,
	case when &amp;LocalCurrency then Balances.OverpaymentBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.OverpaymentBalance
	end as Overpayment,
	case when &amp;LocalCurrency then Balances.BillBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.BillBalance
	end as BillBalance
	{select StrTotalsGroup, StrPayment, StrOverduePayment, Vendor.* as Vendor, Contract.*, Document.*, Detail.*, PaymentDate, PaymentBalance, BalanceDays,
	OverduePeriods, AmountDebt, Overpayment, BillBalance, PaymentKey, Option.*}
from AccumulationRegister.VendorDebts.Balance ( &amp;Asof, {Contract.Company.* as Company, Contract.Owner.* as Vendor, Contract.* as Contract, Document.* as Document,
	Detail.* as Detail, Contract.Currency.* as Currency} ) as Balances
	//
	// PaymentDetails
	//
	left join InformationRegister.PaymentDetails as PaymentDetails
	on PaymentDetails.PaymentKey = Balances.PaymentKey
	//
	// Rates
	//
	left join Rates as Rates
	on Rates.Currency = Balances.Contract.Currency
	//
	// LocalCurrency
	//
	left join Constants as Constants
	on true
{where Balances.PaymentBalance as PaymentBalance, PaymentDetails.Date as PaymentDate, PaymentDetails.Option.* as Option,
	case
		when datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) &lt;= 0 then 0
		when datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) &gt; &amp;PeriodsBorder then &amp;PeriodsBorder + 1
		else &amp;PeriodFactor * (
			case when cast ( datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) / &amp;PeriodFactor - 0.5 as Number ( 15 ) ) = datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) / &amp;PeriodFactor then 0
			else 1
			end + cast ( datediff ( PaymentDetails.Date, &amp;DebtsOnDate, day ) / &amp;PeriodFactor - 0.5 as Number ( 15 ) )
		)
	end as OverduePeriods,
	case when &amp;LocalCurrency then Balances.AmountBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.AmountBalance
	end as AmountDebt,
	case when &amp;LocalCurrency then Balances.OverpaymentBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.OverpaymentBalance
	end as Overpayment}
</query>
