		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:ExpressionAreaTemplateParameter">
			<dcsat:name>OverduePeriods</dcsat:name>
			<dcsat:expression>case
    when OverduePeriods &gt; &amp;PeriodsBorder then &amp;StrOver + " " + Format ( &amp;PeriodsBorder, "" )
    when OverduePeriods &lt; &amp;PeriodFactor then ""
    else Format ( OverduePeriods - &amp;PeriodFactor + 1, "" ) + " - " + Format ( OverduePeriods, "" )
end</dcsat:expression>
		</parameter>
