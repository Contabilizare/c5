		<dataSource>DataSource1</dataSource>
		<query>select Cost.Recorder as CostRecorder, Cost.ItemKey as CostItemKey, sum ( Cost.Amount ) as CostTotal,
	sum ( case Cost.Dependency when undefined then Cost.Amount else 0 end ) as CostBase,
	cast ( sum ( Cost.Amount ) / sum ( Cost.Quantity ) as Number ( 15, 2 ) ) as CostUnit
from AccumulationRegister.Cost as Cost
{where Cost.Recorder as Ref}
group by Cost.Recorder, Cost.ItemKey</query>
