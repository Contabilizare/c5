Procedure fill ( Code, Description, Alpha2Code, Alpha3Code )

	With ( "Countries" );
	try
		if ( GotoRow ( "#List", "Code", Code, true ) ) then
			return;
		endif;
	except// empty list
	endtry;	
	Click ( "#FormCreate" );
	form = With ( "Countries (*" );
	Put ( "#Code", Code );
	Click ( "Yes", Forms.Get1C () );
	Put ( "#Code", Code );
	Put ( "#Description", Description );
	Put ( "#Alpha2Code", Alpha2Code );
	Put ( "#Alpha3Code", Alpha3Code );
	Click ( "#FormWriteAndClose" );
	

EndProcedure